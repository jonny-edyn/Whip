<a href="" id="fb_share">Share On FB</a><br>
<a class="twitter popup" href="http://twitter.com/share?text=I%20just%20voted%20on%20Whip!%20Check%20it%20out%20at%0D%0A&url=http://whip-production.herokuapp.com/">Share On TW</a>
<script type="text/javascript">
	$('body').on('click','#fb_share',function(e){
		e.preventDefault();

		FB.ui(
			 {
			  method: 'feed',
			  href: 'http://whip-production.herokuapp.com/',
			  caption: 'I just voted on Whip!',
			}, function(response){});
	});

	$('body').on('click','#tw_share',function(e){
		e.preventDefault();

		$('.twitter-share-button').trigger('click');

	});

	 $('.popup').click(function(event) {
	    var width  = 575,
	        height = 400,
	        left   = ($(window).width()  - width)  / 2,
	        top    = ($(window).height() - height) / 2,
	        url    = this.href,
	        opts   = 'status=1' +
	                 ',width='  + width  +
	                 ',height=' + height +
	                 ',top='    + top    +
	                 ',left='   + left;
	    
	    window.open(url, 'twitter', opts);
	 
	    return false;
	  });
</script>