<div class="row admin-content">
	<div class="col-md-9" role="main">
		<div class="col-md-12">
			<h2 class="text-din text-uppercase">USERS</h2>
		</div>
		<div class="col-md-12">
			<% if @users.any? %>
				<% @users.each do |user| %>
					<div class="form-group">
					<%= user.email %>
					</div>
				<% end %>
			<% end %>
		</div>
	</div>
	<%= render "admins/admin_sidebar" %>
</div>