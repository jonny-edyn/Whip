<div class="row">
	<div class="col-md-8">
		<div class="row">
			<div class="col-md-12">
				<h2>PARTIES</h2>
			</div>
			<div class="col-md-12">
				<% if @parties.any? %>
					<% @parties.each do |party| %>
						<%= party.name %><br>
					<% end %>
				<% end %>
			</div>
			<div class="col-md-12">
				<h2>Add Party</h2>
			</div>
			<div class="col-md-12">
				<%= form_for @party do |f| %>
					<%= f.text_field :name %>
					<%= f.submit "Add New Party" %>
				<% end %>
			</div>
		</div>
	</div>
	<%= render "admins/admin_sidebar" %>
</div>